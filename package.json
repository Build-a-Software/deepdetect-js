{
  "name": "deepdetect-js",
  "version": "1.5.3",
  "description": "DeepDetect JS client",
  "license": "MIT",
  "author": {
    "name": "Alexandre Girard",
    "email": "git@alexgirard.com",
    "url": "http://deepdetect.com"
  },
  "repository": "https://github.com/jolibrain/deepdetect-js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "nps --no-scripts",
    "precommit": "remove-lockfiles && lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "nps test",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "web-example": "node doc/web-example/server.js"
  },
  "main": "dist/deepdetect.js",
  "files": [
    "dist"
  ],
  "browser": {
    "./index.js": "./dist/deepdetect-browser.js"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "all-contributors-cli": "^4.5.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "12.0.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "2.7.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.2.0",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^4.2.3",
    "nps": "^5.7.1",
    "nps-utils": "^1.4.0",
    "prettier": "^1.7.4",
    "remove-lockfiles": "^1.4.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "semantic-release": "^15.3.1",
    "travis-deploy-once": "^5.0.0",
    "uglify-es": "^3.3.9"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "deepdetect",
    "deep-learning",
    "machine-learning",
    "rest-api",
    "client",
    "javascript",
    "request"
  ]
}
