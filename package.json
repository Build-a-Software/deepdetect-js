{
  "name": "deepdetect-js",
  "version": "0.0.0-development",
  "description": "DeepDetect JS client",
  "license": "MIT",
  "author": {
    "name": "Alexandre Girard",
    "email": "git@alexgirard.com",
    "url": "http://deepdetect.com"
  },
  "repository": "https://github.com/jolibrain/deepdetect-js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "nps --no-scripts",
    "precommit": "remove-lockfiles && lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "nps test",
    "doc": "cd src; groc index.js -o ../doc",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "groc": "^0.8.0",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "all-contributors-cli": "^4.5.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "12.0.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "2.7.0",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "nps": "^5.7.1",
    "nps-utils": "^1.4.0",
    "prettier": "^1.7.4",
    "remove-lockfiles": "^1.4.1",
    "semantic-release": "^15.3.1",
    "travis-deploy-once": "^5.0.0",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.3.24"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "deepdetect",
    "deep-learning",
    "machine-learning",
    "rest-api",
    "client",
    "javascript",
    "request"
  ]
}
